{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore",
            "allow-trailing-underscore",
            "allow-pascal-case",
            "allow-snake-case"
        ],
        "curly": false,
        "array-type": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "quotemark": [
            false
        ],
        "no-console": [
            false
        ],
        "trailing-comma": [
            false
        ],
        "arrow-parens": false,
        "object-literal-shorthand": false,
        "member-access": [
            "check-constructor"
        ],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}