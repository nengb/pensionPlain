#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/ts-node" ]; then
  "$basedir/ts-node"  "$basedir/../wechaty/dist/bin/puppet-install.js" "$@"
  ret=$?
else 
  ts-node  "$basedir/../wechaty/dist/bin/puppet-install.js" "$@"
  ret=$?
fi
exit $ret
